{
  "rules": {
    "h": {
      "$id": {
        ".read": true
      }
    },

    "t": {
      "$kelurahanId": {
        "$tpsNo": {
          ".read": true
        }
      }
    },

    "uploads": {
      "$imageId": {
        "userId": {
          ".read": true
        }
      }
    },

    "upserts": {
      "data": {
        ".indexOn": "t"
      }
    },

    "kelurahan": {
      "$id": {
        "tps": {
          "$tpsNo": {
            ".read": true
          }
        }
      }
    }
  }
}
